#ifndef LADDER_H_
#define LADDER_H_

#include <stdlib.h>
#include <stdint.h>
#include <glob.h>

struct HardItem
{
	uint16_t midlist[62500];
	struct SoftItem* soft_next;
	struct HardItem* hard_next;
};
typedef struct HardItem HardItem;

struct SoftItem
{
	char* disk_location;
	struct SoftItem* soft_next;
	struct HardItem* hard_next;
};

#endif /* LADDER_H_ */
